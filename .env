# Webhook service config
PORT=8080

# PostgreSQL connection
POSTGRES_DSN=postgres://webhook:webhook@postgres:5432/webhookdb?sslmode=disable

# Stellar Horizon stream
HORIZON_STREAM_URL=https://horizon.stellar.org/operations?cursor=now&stream=true

# NATS (JetStream) configuration
NATS_URL=nats://nats:4222
NATS_STREAM=stellar-events
NATS_CONSUMER=webhook-consumer